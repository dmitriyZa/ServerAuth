@page
@model IdentityServerAspNetIdentity.Pages.Logout.Index

<div class="logout-page">
    <div class="lead">
        <h1>Выход из системы</h1>
        <p>Требуется выход из системы?</p>
    </div>

    <form asp-page="/Account/Logout/Index">
        <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
        
        <div class="form-group">
            <button class="btn btn-primary">Да</button>
        </div>
    </form>
</div>